{
  "name": "cypress_demoblaze",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "jsaleem",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "ui-regression": "cypress run --browser chrome --spec \"cypress/e2e/AllTest/**/*.{cy.js,js}\"",
    "ui-regression-allure": "cypress run --browser chrome --spec \"cypress/e2e/**/*.{cy.js,js}\" --env allure=true",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true",
    "allure:open": "allure open allure-report"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "allure-cypress": "^3.4.1",
    "cypress": "^15.7.1",
    "cypress-xpath": "^2.0.1"
  }
}
